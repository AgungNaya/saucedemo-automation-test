{
    "name": "selenium-mocha-digital-skola",
    "version": "1.0.0",
    "description": "Selenium WebDriver tests using Mocha with Mochawesome reports",
    "scripts": {
        "testing": "mocha tests/test-sort.js --recursive --timeout 60000",
        "jalankan-mochawesome": "mocha tests/test-sort.js --recursive --timeout 60000 --reporter mochawesome",
        "jalankan-simple-html": "mocha tests/test-sort.js --recursive --timeout 60000 --reporter mocha-simple-html-reporter --reporter-options output=report-test.html"
    },
    "dependencies": {
        "selenium-webdriver": "^4.17.0"
    },
    "devDependencies": {
        "mocha": "^10.4.0",
        "mochawesome": "^7.1.3",
        "mocha-simple-html-reporter": "2.0.0"
    }
}